{
  "name": "action-azure-upload-blob",
  "version": "1.10.0",
  "private": false,
  "description": "GitHub Acton that uploads file to Azure Storage blob container.",
  "main": "lib/main.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc",
    "rebuild": "rimraf lib && tsc",
    "format": "prettier --write **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "set NODE_OPTIONS=--openssl-legacy-provider && ncc build --source-map --license licenses.txt",
    "test": "jest --coverage",
    "pre-commit": "npm run rebuild && npm run format && npm run lint && npm run package",
    "all": "npm run rebuild && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LanceMcCarthy/Action-AzureBlobUpload.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Lance McCarthy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LanceMcCarthy/Action-AzureBlobUpload/Issues"
  },
  "homepage": "https://github.com/LanceMcCarthy/Action-AzureBlobUpload#readme",
  "dependencies": {
    "@actions/core": "^1.6.0",
    "@azure/storage-blob": "^12.8.0",
    "@types/glob": "^7.2.0",
    "@types/mime-types": "^2.1.1",
    "glob": "^7.2.0",
    "mime-types": "^2.1.34"
  },
  "devDependencies": {
    "@types/node": "^17.0.14",
    "@typescript-eslint/parser": "^5.10.2",
    "@vercel/ncc": "^0.33.1",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.5",
    "eslint-plugin-i18n-text": "^1.0.1",
    "eslint-plugin-jest": "^26.0.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.0",
    "js-yaml": "^4.1.0",
    "node-notifier": "^10.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tmpl": "^1.0.5",
    "typescript": "^4.5.5"
  }
}
